<%- load_pools(search: input('Subscriptions filter'), includes: [:subscription, :products]).each_record do |pool| -%>
<%-   report_row(
        'ID': pool.id,
        'Name': pool.name,
        'Available': pool.available,
        'Quantity': pool.quantity,
        'SKU': pool.product_id,
        'Contract number': pool.contract_number,
      ) -%>
<%- end -%>
<%= report_render -%>
